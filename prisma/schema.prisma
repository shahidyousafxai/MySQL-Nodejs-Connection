generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model test {
  id   Int    @id @default(autoincrement())
  name String @db.VarChar(100)
  city String @db.VarChar(100)
}

model Test1 {
  id Int @id @default(autoincrement())
  name String @db.VarChar(100)
  city String @db.VarChar(100)
}

model test2 {
  id   Int    @id @default(autoincrement())
  name String @db.VarChar(200)
  city String @db.VarChar(100)
}
